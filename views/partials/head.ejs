                }
              
                #home-bottom{
                    grid-template-columns: repeat(13, 1fr);
                }
                #home-bottom div:nth-child(1){
                    grid-column: 2/5;
                    grid-row: 1;
                }
                #home-bottom div:nth-child(2){
                    grid-column: 4/7;
                    grid-row: 2;
                }
                #home-bottom div:nth-child(3){
                    grid-column: 8/10;
                    grid-row: 2;
                    
                }
                #home-bottom div:nth-child(4){
                    grid-column: 8/11;
                    grid-row: 3;
                }
               
                .space-between{
                    display: inline-block;
                    padding-right: 1em;
                }
                #services-top div:nth-child(2){
                    grid-column: 5/11;
                }
                #services-top div h5{
                    font-size: small;
                }
                #services-top div h2{
                    font-size: xx-large
                }
                #services-top div h6{
                    font-size: small;
                }
                #gallery{
                    grid-gap: 20px 20px;
                }
                #gallery div:nth-child(1){
                    grid-column: 2/8;
                    grid-row: 1;
                    
                }
                #gallery div:nth-child(2){
                    grid-column: 8/14;
                    grid-row: 1;
                    
                }
                #gallery div:nth-child(3){
                    grid-column: 2/8;
                    grid-row: 2;
                    
                }
                #gallery div:nth-child(4){
                    grid-column: 8/14;
                    grid-row: 2;
                    
                }
                #gallery div:nth-child(5){
                    grid-column: 2/8;
                    grid-row: 3;
                   
                }
                #gallery div:nth-child(6){
                    grid-column: 8/14;
                    grid-row: 3;
                   
                }
                #gallery div:nth-child(7){
                    grid-column: 2/8;
                    grid-row: 4;
                    
                }
                #gallery-portrait{
                   
                }
            
                #gallery-portrait div:nth-child(1){
                grid-column: 2/6;
                grid-row: 1;
                padding-bottom: 20px;
               
            }
            #gallery-portrait div:nth-child(2){
                grid-column: 6/10;
                grid-row: 1;
                
            }
            #gallery-portrait div:nth-child(3){
                grid-column: 10/14;
                grid-row: 1;
                
                
            }
            #gallery-portrait div:nth-child(4){
                grid-column: 2/6;
