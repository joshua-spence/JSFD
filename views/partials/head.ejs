
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>JSFD</title>
    <link href="https://fonts.googleapis.com/css2?family=Archivo:wght@400;500;600;700&family=Playfair+Display:wght@400;500&family=Poppins:wght@400;500&display=swap" rel="stylesheet">
    <script src="https://kit.fontawesome.com/a076d05399.js"></script>
    <style>
        *{
            box-sizing: border-box;
            margin: 0;
        }

        body{
            background-color: #f2fff5;
        }
        h1{
            font-family: 'Playfair Display', serif;
            font-weight: 500;
        }
        h2{
            font-family: 'Playfair Display', serif;
            font-weight: 500;
           
        }
        
        h3{
            font-family: 'Playfair Display', serif;
            font-weight: 500;
           
        }
        h4{
            font-family: 'Poppins', sans-serif;
            font-weight: 400;
        }
        h5{
            font-family: 'Poppins', sans-serif;
            font-weight: 400;
            
        }
        h6{
            font-family: 'Poppins', sans-serif;
            font-weight: 400;
        }
        p{
            font-family: 'Poppins', sans-serif;
        }

        /* menu for full screen */

            nav{
                width: 100%;
                height: 75px;
                background: #f2fff5;
                
            }
            nav li{
                list-style: none;
            }
            nav li a{
                font-size: smaller;
                font-family: 'Poppins', sans-serif;
                font-weight:500;
                text-decoration: none;
                color: black;
            }
            nav ul{
                float: right;
                margin-right: 10px;
            }
            nav ul li{
                display: inline-block;
                line-height: 70px;
                margin: 0 25px;
            }
            .logo{
                /* margin-left: 85px; */
                padding: 10px;
            }
            .logo img{
                padding-top: 10px;
                width: 60px;
                
                margin-left: 1.7vw;
            }
            .checkbtn{
                font-size: 30px;
                color: black;
                float:right;
                line-height: 80px;
                margin-right: 30px;
                cursor: pointer;
                display: none;
            }
            #check{
                display: none;
            }
        
        /* menu for smaller screen  */

        @media (max-width: 1020px){
            nav{
                height: 70px;
                position: relative;
            }
            .checkbtn{
                display: block;
                font-size: 17px;
                padding-top: 23px;
            }
            .logo img{
                width: 60px;
                padding-top: 10px;
                margin-left: 3px;
            }
            ul{
                position: fixed;
                width: 30%;
                height: 100vh;
                background: white;
                top: 0px;
                left: -100%;
                text-align: left;
                z-index: 7;
                transition: all .5s;
            }
            nav ul li{
                display: block;
                margin: 50px 0;
                line-height: 30px;
            }
            nav ul li a{
                font-size: 8px;
            }
            #check:checked ~ ul{
                left: 0;
            }
            #check{
                display: none;
            }
        } 
   
        footer{
            display: grid;
            position: relative;
            grid-template-columns: repeat(12, 1fr);
            grid-template-rows: auto;
            width: 100%;
            bottom: 0;
            padding: 20px 0;
        }
        footer div{
            grid-column: 2/12;
            grid-row: 1;
        }
        footer div h4{
            font-size: 12px;
            font-weight: 400;
            text-align: center;
            padding: 5px;
        }
        footer div h4 a{
            text-decoration: none;
            color: black;
        }

        /* effects */
        .bullet-point-1{
            visibility: hidden;
        }
        .bullet-point-2{
            visibility: hidden;
        }
        .bullet-point-3{
            visibility: hidden;
        }
        .bullet-point-4{
            visibility: hidden;
        }
        .bullet-point-5{
            visibility: hidden;
        }
        .bullet-point-6{
            visibility: hidden;
        }
        .occassions-photo-1{
            visibility: hidden;
        }
        .occassions-photo-2{
            visibility: hidden;
        }
        .occassions-photo-3{
            visibility: hidden;
        }
    
        /* smaller screen  */
        #home-top{
                display: grid;
                grid-template-columns: repeat(14, 1fr);
                grid-template-rows: auto;
                width: 100%;
                padding-bottom: 130px
            }
            #home-top div:nth-child(1){
                visibility: hidden;
                grid-column: 4/12;
                grid-row: 5;
                text-align: center;
                margin: 15px 10px;
                padding: 20vh 0;
            }
            #home-top div:nth-child(2){
                grid-column: 1/15;
                grid-row: 1/4;
                z-index: -2;
                margin: 0 15px;
                padding-bottom: 150px;
                height: 100vh;
                background: url('../images/home/top-photo-2.JPG');
                background-size: cover;
            }
            #home-top div:nth-child(3){
                grid-column: 4/12;
                grid-row: 7;
                text-align: left;
                
            }
            #home-top div:nth-child(4){
                grid-column: 4/12;
                grid-row: 6;
                padding-bottom: 30px;
                
                
            }
            #home-top div h3{
                /* text-transform: uppercase; */
                padding-bottom: 15px;
            }
           
            .list-photo{
                width: 100%;
            }
            #home-bottom{
                display: grid;
                grid-template-columns: repeat(14, 1fr); 
                background-color: white;               
                padding-top: 100px;
                padding-bottom: 60px;
            }

            .space-between{
                display: inline-block;
                padding-right: 1em;
                padding-top: 15px;
            }

            #home-bottom div:nth-child(1){
                grid-column: 2/13;
                grid-row: 1;
                text-align: left;
                padding-bottom: 60px;
            }
            #home-bottom div:nth-child(2){
                grid-column: 4/14;
                grid-row: 2;
                text-align: left;
                padding-bottom: 60px;
            }
            #home-bottom div:nth-child(3){
                grid-column: 2/12;
                grid-row: 3;
                text-align: left;
                padding-bottom: 60px;

            }
            #home-bottom div:nth-child(4){
                grid-column: 3/13;
                grid-row: 4;
                padding-bottom: 40px;

            }
            #home-bottom div p{
                padding-top: 5px;
                
            }
            .occasions img{
                width: 100%;
            }
            #services-top{
                display: grid;
                grid-template-columns: repeat(14, 1fr);
            }
            #services-top div:nth-child(1){
                grid-column: 2/14;
                grid-row: 1;
                
                text-align: center;
                border-bottom: 1px solid black;
            }
            #services-top div:nth-child(2){
                grid-column: 3/13;
                grid-row: 2;
                text-align: center;
            }
            #services-top div:nth-child(3){
                grid-column: 3/13;
                grid-row: 3;
                text-align: center;
                padding-bottom: 100px;
            }
            #services-top div h5{
                padding: 30px 0;
            }
            #services-top div h2{
                padding: 40px 0;
            }
            #gallery{
                display: grid;
                grid-template-columns: repeat(14, 1fr);
                grid-gap: 30px 0px;
                
            }
            #gallery div:nth-child(1){
                grid-column: 3/13;
                grid-row: 1;
            }
            #gallery div:nth-child(2){
                grid-column: 3/13;
                grid-row: 2;
            }
            #gallery div:nth-child(3){
                grid-column: 3/13;
                grid-row: 3;
            }
            #gallery div:nth-child(4){
                grid-column: 3/13;
                grid-row: 4;
            }
            #gallery div:nth-child(5){
                grid-column: 3/13;
                grid-row: 5;
            }
            #gallery div:nth-child(6){
                grid-column: 3/13;
                grid-row: 6;
            }
            #gallery div:nth-child(7){
                grid-column: 3/13;
                grid-row: 7;
            }
            #gallery div img{
                width: 100%;
            }
            #gallery-portrait{
                display: grid;
                grid-template-columns: repeat(14, 1fr);
                grid-gap: 30px 0;
                
            }
            #gallery-portrait div:nth-child(1){
                grid-column: 4/12;
                grid-row: 1;
            }
            #gallery-portrait div:nth-child(2){
                grid-column: 4/12;
                grid-row: 2;
            }
            #gallery-portrait div:nth-child(3){
                grid-column: 4/12;
                grid-row: 3;
            }
            #gallery-portrait div:nth-child(4){
                grid-column: 4/12;
                grid-row: 4;
            }
            #gallery-portrait div:nth-child(5){
                grid-column: 4/12;
                grid-row: 5;
            }
            #gallery-portrait div:nth-child(6){
                grid-column: 4/12;
                grid-row: 6;
            }
            #gallery-portrait div:nth-child(7){
                grid-column: 4/12;
                grid-row: 7;
            }
            #gallery-portrait div:nth-child(8){
                grid-column: 4/12;
                grid-row: 8;
            }
            #gallery-portrait div h4{
                font-size: 14px;
                font-weight: 400;
                font-family: 'Montserrat', sans-serif;
            }
            #gallery-portrait div img{
                width: 100%;
            }

            #aboutUs{
                display: grid;
                grid-template-columns: repeat(12, 1fr);
                grid-gap: 30px 0px;
                padding-top: 60px;
            }
            #aboutUs div:nth-child(1){
                grid-column: 4/10;
                grid-row: 2;
                
            }
            #aboutUs div:nth-child(2){
                grid-column: 2/13;
                grid-row: 1;
                /* padding-left: 15px; */
                padding-right: 35px;
                
            }
            /* #aboutUs div:nth-child(3){
                grid-column: 3/11;
                grid-row: 5;
                text-align: center;
                padding-bottom: 60px;
                
            } */
            #aboutUs div h3{
                padding-bottom: 15px;
                text-align: center;
                border-bottom: 1px black solid;
            }
            #aboutUs div h5{
                padding-top: 20px;
            }
            #aboutUs div img{
                width: 100%;
            }

            /* smaller tablet  */

            @media screen and (min-width: 650px){
