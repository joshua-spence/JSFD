            }
            #services-top div h6{
                margin: 0 50px;
            }

            #gallery-portrait{
                grid-gap: 5px 20px;
            }            
            #gallery-portrait div:nth-child(1){
                grid-column: 3/8;
                grid-row: 1;
            }
            #gallery-portrait div:nth-child(2){
                grid-column: 8/13;
                grid-row: 1;
            }
            #gallery-portrait div:nth-child(3){
                grid-column: 3/8;
                grid-row: 2;
            }
            #gallery-portrait div:nth-child(4){
                grid-column: 8/13;
                grid-row: 2;
            }
            #gallery-portrait div:nth-child(5){
                grid-column: 3/8;
                grid-row: 3;
            }
            #gallery-portrait div:nth-child(6){
                grid-column: 8/13;
                grid-row: 3;
                
            }
            #gallery-portrait div:nth-child(7){
                grid-column: 3/8;
                grid-row: 4;
               
            }
            #aboutUs div:nth-child(1){
                grid-column: 2/6;
                grid-row: 1;
                
            }
            #aboutUs div:nth-child(2){
                padding-top: 50px;
                grid-column: 7/13;
                grid-row: 1;
            }
            #aboutUs div h3{
                border: none;
                text-align: left;
            }
            #aboutUs div h5{
                padding-top: 0px;
            }
            
            }

            /* desktop screen */

            @media screen and (min-width: 850px){
 
                #home-top div:nth-child(1){
                    grid-column: 5/11;
                }
                #home-top div:nth-child(2){
